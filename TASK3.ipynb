   "source": [
    "# pattern from * using a nested for loop.\n",
    "\n",
    "X=5;\n",
    "for Y in range(X):\n",
    "    for j in range(Y):\n",
    "        print ('* ', end=\"\")\n",
    "    print('')\n",
    "\n",
    "for Y in range(X,0,-1):\n",
    "    for j in range(Y):\n",
    "        print('* ', end=\"\")\n",
    "    print('')"
   ]
  }
